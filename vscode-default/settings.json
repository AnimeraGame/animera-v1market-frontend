{
  "folders": [],
  "settings": {},
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "eslint.options": {
    "env": {
      "est": true
    },
    "parseOptions": {
      "ecmaVersion": 6
    }
  },
  "jshint.options": {
    "asi": true,
    "esversion": 6
  },
  "files.associations": {
    "*.js": "javascriptreact"
  },
  "editor.tabSize": 2,

  // cSpell Plugin
  "cSpell.words": [
    "antd",
    "combinator",
    "flowtype",
    "refetch",
    "sizeme",
    "styleguidist",
    "stylelint",
    "themeable",
    "uiclient",
    "unmount"
  ],
  "cSpell.language": "en",
  "eslint.alwaysShowStatus": true,
  // Prettier Plugin Support - Copy Commented lines to Workspace Settings
  "javascript.validate.enable": false,
  //Standard Plugin Support
  "standard.usePackageJson": true,
  "editor.codeActionsOnSave": {
    // Eslint Plugin
    "source.fixAll.eslint": true
    // "javascript.format.enable": false,
    //"source.organizeImports": true
  },
